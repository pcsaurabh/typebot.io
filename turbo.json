{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "globalDependencies": [".env"],
  "globalEnv": ["SKIP_ENV_CHECK"],
  "globalPassThroughEnv": ["ENCRYPTION_SECRET"],
  "tasks": {
    "lint": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "env": ["VERCEL_*", "NEXTAUTH_URL", "SENTRY_*", "LANDING_PAGE_URL"],
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "build/**",
        "public/__ENV.js"
      ],
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "outputLogs": "new-only"
    }
  }
}
